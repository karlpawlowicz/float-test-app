{
  "name": "float-test-app",
  "version": "0.0.1",
  "description": "A Next.js starter that you can deploy to AWS",
  "repository": {
    "type": "git",
    "url": "https://github.com/karlpawlowicz/float-test-app.git"
  },
  "author": "Karl Pawlowicz <karlpawlowicz@gmail.com> (https://karl.dev/)",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://github.com/karlpawlowicz/float-test-app#readme",
  "bugs": {
    "url": "https://github.com/karlpawlowicz/float-test-app/issues"
  },
  "scripts": {
    "build": "next build",
    "deploy": "serverless deploy",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{cjs,css,js,json,jsx,md,mjs,scss,ts,tsx,yml}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install && npx next telemetry disable",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "type-check": "tsc"
  },
  "dependencies": {
    "next": "^12.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.3",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "serverless": "^3.22.0",
    "serverless-cloudfront-invalidate": "^1.12.2",
    "serverless-s3-sync": "^3.0.0",
    "typescript": "^4.8.3"
  },
  "lint-staged": {
    "*{cjs,js,jsx,mjs,ts,tsx}": [
      "eslint ."
    ],
    "*.{cjs,css,js,json,jsx,md,mjs,scss,ts,tsx,yml}": [
      "prettier --write"
    ]
  }
}
